{
    "name": "zalehy/ws-configurator-api-sdk-php",
    "version": "2.0.0",
    "description": "Wheel Configurator API PHP client SDK",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "WheelSizeApiClient\\Configurator\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "WheelSizeApiClient\\Configurator\\" : "test/" }
    }
}
